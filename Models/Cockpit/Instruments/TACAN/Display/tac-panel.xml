<?xml version="1.0"?>
<PropertyList>

<name>MFD Panel</name>
<background>Aircraft/747-8i/Models/Cockpit/Instruments/PFD/transparent.png</background>
<w>256</w>
<h>256</h>

<instruments>
    <instrument include="MAP.xml">
        <condition>
            <and>
                <greater-than>
                    <property>systems/electrical/outputs/efis</property>
                    <value>15</value>
                </greater-than>
                <equals>
                    <property>instrumentation/efis/mfd/display-mode</property>
                    <value>MAP</value>
                </equals>
            </and>
        </condition>
        <name>map</name>
        <x>128</x>
        <y>128</y>
        <w>256</w>
        <h>256</h>
    </instrument>
	
</instruments>

</PropertyList>
